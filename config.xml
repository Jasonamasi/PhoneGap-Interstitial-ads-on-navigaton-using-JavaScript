<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget xmlns       =    "http://www.w3.org/ns/widgets"
        xmlns:gap   =    "http://phonegap.com/ns/1.0"
        id          =     "com.pointdeveloper.allcouriertracking"
        versionCode =   "1"
        version     =    "1.0.0">
  <name>Phonegap Buil Admob demo</name>
  <description>
 App to check admob ads.
    </description>
  <author email="news@pointdeveloper.com" href="http://pointdeveloper.com/">
      Prantik vaghela
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  
  <gap:platform name="android" />
  <preference name="phonegap-version" value="cli-6.0.0" />
  <preference name="orientation" value="portrait" />
  <preference name="fullscreen" value="true" />
  <preference name="KeepRunning" value="false"/>
  <preference name="android-minSdkVersion" value="16" />
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="3000" />
  <preference name="permissions" value="none"/>

  <preference name="android-build-tool" value="gradle" /> 
  <plugin name="cordova-plugin-whitelist" source="npm" />
  <plugin name="cordova-plugin-admobpro" source="npm" />
 
 
</widget>
